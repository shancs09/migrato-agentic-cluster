    "components": {
        "schemas": {
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "Overview": {
                "properties": {
                    "total_documents": {
                        "type": "integer",
                        "title": "Total Documents"
                    },
                    "total_clusters": {
                        "type": "integer",
                        "title": "Total Clusters"
                    },
                    "labeled_clusters": {
                        "type": "integer",
                        "title": "Labeled Clusters"
                    },
                    "unlabeled_clusters": {
                        "type": "integer",
                        "title": "Unlabeled Clusters"
                    },
                    "coverage_percent": {
                        "type": "number",
                        "title": "Coverage Percent"
                    },
                    "data_source": {
                        "type": "string",
                        "title": "Data Source"
                    }
                },
                "type": "object",
                "required": [
                    "total_documents",
                    "total_clusters",
                    "labeled_clusters",
                    "unlabeled_clusters",
                    "coverage_percent",
                    "data_source"
                ],
                "title": "Overview"
            },
            "ReadDataResponse": {
                "properties": {
                    "overview": {
                        "$ref": "#/components/schemas/Overview"
                    },
                    "doc_label_status_distribution": {
                        "additionalProperties": {
                            "type": "integer"
                        },
                        "type": "object",
                        "title": "Doc Label Status Distribution"
                    },
                    "unlabeled_cluster_ids": {
                        "items": {
                            "type": "integer"
                        },
                        "type": "array",
                        "title": "Unlabeled Cluster Ids"
                    }
                },
                "type": "object",
                "required": [
                    "overview",
                    "doc_label_status_distribution",
                    "unlabeled_cluster_ids"
                ],
                "title": "ReadDataResponse"
            },
            "UnlabeledClustersResponse": {
                "properties": {
                    "unlabeled_cluster_ids": {
                        "items": {
                            "type": "integer"
                        },
                        "type": "array",
                        "title": "Unlabeled Cluster Ids"
                    }
                },
                "type": "object",
                "required": [
                    "unlabeled_cluster_ids"
                ],
                "title": "UnlabeledClustersResponse"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            }
        }
    }
}